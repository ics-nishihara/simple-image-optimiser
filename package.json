{
  "name": "simple-image-optimiser",
  "version": "1.0.0",
  "repository": "git@github.com:ics-nishihara/simple-image-optimiser.git",
  "author": "ics-nishihara <nishihara@ics-web.jp>",
  "scripts": {
    "convert-all":"yarn run convert-jpg-to-webp || yarn run convert-png-to-webp",
    "convert-jpg-to-webp": "yarn run squoosh-cli --webp '{quality: 75}' images/*.jpg -d dist",
    "convert-png-to-webp": "yarn run squoosh-cli --webp '{quality: 75}' images/*.png -d dist",
    "optimise-jpg": "yarn run squoosh-cli --mozjpeg '{quality: 75}' images/*.jpg -d dist",
    "optimise-png": "yarn run squoosh-cli --oxipng '{level: 2}' images/*.png -d dist"
  },
  "dependencies": {
    "@squoosh/cli": "0.6.3"
  }
}
